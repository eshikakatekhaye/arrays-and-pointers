//Accessing the elements of array using pointers.
#include<iostream>
using namespace std;
int main()
{
    int arr[5];
    cout<<"Enter elements: ";
    for(int i=0 ; i<5 ; i++)
    {
        cin>>arr[i];
    }
        int *p;
        p=arr;
        cout<<"\nYou entered: ";
        for(int i=0 ; i<5 ; i++)
        {
            cout<<*p<<"\n";
            p++;
        }
    return 0;
}
